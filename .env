# JWT 설정
SECRET_KEY=your-secret-key-here  # Access Token 서명에 사용되는 비밀 키
REFRESH_SECRET_KEY=your-refresh-secret-key-here  # Refresh Token 서명에 사용되는 비밀 키
ALGORITHM=HS256  # JWT 토큰 암호화 알고리즘

# 토큰 만료 시간 설정
ACCESS_TOKEN_EXPIRE_MINUTES=30  # Access Token 만료 시간 (분)
REFRESH_TOKEN_EXPIRE_DAYS=30  # Refresh Token 만료 시간 (일)